{
  "extends": [
    "config:recommended"
  ],
  "automerge": false,
  "labels": ["dependencies"],
  "dependencyDashboard": true,
  "packageRules": [
    {
      "matchManagers": ["nuget"],
      "enabled": true,
      "automerge": true,
      "automergeType": "pr"
    }
  ],
  "packageGroups": {
    "discatsharp": {
      "matchPackagePrefixes": ["DisCatSharp."],
      "groupName": "DisCatSharp libraries"
    }
  }
}
